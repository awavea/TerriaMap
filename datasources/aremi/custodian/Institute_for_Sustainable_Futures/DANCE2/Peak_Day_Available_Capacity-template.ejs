    <tr>
        <td>Hour                                        </td>
        <td colspan='2'>Available Capacity              </td>
        <td>Hourly exceedance share                     </td>
    </tr>
    <tr>
        <td><small>Units</small>                        </td>
        <td><small>MVA</small>                          </td>
        <td><small>% of capacity</small>                </td>
        <td><small>% of total annual exceedance</small> </td>
    </tr>
{{#HH}}
    <tr>
        <td>{{Hour}}                    </td>
        <td>{{Available_capacity}}      </td>
        <td>{{Available_capacity_perc}} </td>
        <td>{{Hourly_exceedance_share}} </td>
    </tr>
{{/HH}}
</table>
